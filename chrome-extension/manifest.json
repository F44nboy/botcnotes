{
  "manifest_version": 3,
  "name": "BoTCT Player Extractor",
  "version": "1.1.0",
  "description": "Liest Spielernamen aus botc.app aus.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://botc.app/*"],
  "action": {
    "default_title": "Spieler auslesen"
  },
  "background": {
    "service_worker": "background.js"
  }
}
